<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>sign</servlet-name>
        <servlet-class>guestbook.SignGuestbookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAccount</servlet-name>
        <servlet-class>guestbook.DeleteAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loggingIn</servlet-name>
        <servlet-class>guestbook.LogInPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logInFail</servlet-name>
        <servlet-class>guestbook.LogInFailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signingIn</servlet-name>
        <servlet-class>guestbook.SigningInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>practicePage</servlet-name>
        <servlet-class>guestbook.PracticePage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>guestbook</servlet-name>
        <servlet-class>guestbook.GuestbookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAccount</servlet-name>
        <url-pattern>/deleteAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>practicePage</servlet-name>
        <url-pattern>/practicePage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loggingIn</servlet-name>
        <url-pattern>/loggingIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signingIn</servlet-name>
        <url-pattern>/signingIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>guestbook</servlet-name>
        <url-pattern>/guestbook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logInFail</servlet-name>
        <url-pattern>/logInFail</url-pattern>
    </servlet-mapping>
<welcome-file-list>
        <welcome-file>guestbook.jsp</welcome-file>
    </welcome-file-list>
</web-app>